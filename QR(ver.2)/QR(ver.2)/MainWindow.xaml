<Window x:Class="QR_ver._2_.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QR_ver._2_"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Image x:Name="image" HorizontalAlignment="Left" Height="321" Margin="53,40,0,0" VerticalAlignment="Top" Width="321"/>
        <Button x:Name="openButton" Content="Open" HorizontalAlignment="Left" Margin="420,40,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-1.725,0.242" Click="openButton_Click"/>
        <Button x:Name="createButton" Content="Create" HorizontalAlignment="Left" Margin="420,106,0,0" VerticalAlignment="Top" Width="75" Click="createButton_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="566,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="saveButton" Content="Save" HorizontalAlignment="Left" Margin="420,174,0,0" VerticalAlignment="Top" Width="75" Click="saveButton_Click"/>

    </Grid>
</Window>
